
import 'BasicSpec.my'


BasicSpec {
  name: "Evaluators"
  
  TestEvaluator: BasicObject {
    evaluate: |cscope, data| data.last.map |item| { item[1][1][1] }
  }
  
  subject: Object (TestEvaluator) { var foo, var bar, var baz }
  
  [tests]
  
  "uses an explicitly given custom evaluator": {
    assert_equal(subject, [:foo, :bar, :baz])
  }
}
