
CodeTools::AST << {
  
  DeclareFile < Node {
    node_type declfile
    field body
    
    bytecode: |g| evaluate_bytecode(g)
    
    # TODO: don't be redundant with data_bytecode
    data_array: [:file,
      self.body.array.map |item| { item.data_array },
    ]
    
    data_bytecode: |g| {
      pos(g)
      
      g.push_literal(:file)
        self.body.array.each |item| { item.data_bytecode(g) }
      g.make_array(self.body.array.count)
      g.make_array(2)
    }
  }
  
}
