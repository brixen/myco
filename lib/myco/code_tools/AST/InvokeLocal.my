
CodeTools::AST << {
  
  InvokeLocal < Node {
    field name
    
    const is_data: true
    
    bytecode: |g| {
      pos(g)
      
      local = g.state.scope.search_local(self.name)
      local &? (
        local.get_bytecode(g)
      ) ?? (
        g.push_self
        g.send(self.name, 0)
      )
    }
  }
  
}
