
CodeTools::AST << {
  
  AssignLocal < Node {
    node_type assign_local
    field name, field value
    
    const is_data: true
    
    bytecode: |g| {
      local = g.state.scope.assign_local_reference(self.name)
      
      self.value && self.value.bytecode(g)
      pos(g)
      local.set_bytecode(g)
    }
  }
  
}
