
CodeTools::AST << {
  
  SymbolCompose < Node {
    var body
    
    Form: [:symbol_compose, :body]
    
    bytecode: |g| {
      pos(g)
      
      self.body.each_slice(2) |string, other| {
        g.push_literal(string.value.to_s)
        other && (other.bytecode(g); g.meta_to_s)
      }
      
      g.string_build(self.body.size)
      g.send(:to_sym, 0)
    }
  }
  
}
