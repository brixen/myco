
CodeTools::AST << {
  
  SplatValue < Node {
    node_type splat
    field value
    
    const is_data: true
    
    bytecode: |g| {
      # TODO: determine if coercion from other types is necessary here
      g.push_rubinius
      g.find_const(:Runtime)
        self.value.bytecode(g)
      g.send(:dup_as_array, 1, true)
    }
  }
}
