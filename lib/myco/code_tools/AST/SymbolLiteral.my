
CodeTools::AST << {
  
  SymbolLiteral < Node {
    node_type symbol
    field value
    
    to_literal: self.value
    
    bytecode: |g| {
      pos(g)
      
      g.push_literal(self.value)
    }
    
    # TODO: don't be redundant with data_bytecode
    data_array: [:symbol,
      self.value,
    ]
    
    data_bytecode: |g| {
      g.push_literal(:symbol)
      g.push_literal(self.value)
      g.make_array(2)
    }
  }
  
}
